    <item>
        <name>Active S_SHIFT, D_OPT, F_CMD, J_CMD, K_OPT, L_SHIFT, SEMICOLON_CTRL</name>
        <identifier>private.HAPPY_KEYOUT.HANDY_MODIFIER</identifier>

        <autogen>__KeyOverlaidModifier__ KeyCode::S, KeyCode::SHIFT_L, KeyCode::S</autogen>
        <autogen>__KeyOverlaidModifier__ KeyCode::D, KeyCode::OPTION_L, KeyCode::D</autogen>
        <autogen>__KeyOverlaidModifier__ KeyCode::F, KeyCode::COMMAND_L, KeyCode::F</autogen>
        <autogen>__KeyOverlaidModifier__ KeyCode::J, KeyCode::COMMAND_R, KeyCode::J</autogen>
        <autogen>__KeyOverlaidModifier__ KeyCode::K, KeyCode::OPTION_R, KeyCode::K</autogen>
        <autogen>__KeyOverlaidModifier__ KeyCode::L, KeyCode::SHIFT_R, KeyCode::L</autogen>
        <autogen>__KeyOverlaidModifier__ KeyCode::SEMICOLON, KeyCode::SHIFT_L, KeyCode::SEMICOLON,</autogen>

        <block>
            <autogen>__BlockUntilKeyUp__ KeyCode::S</autogen>
            <autogen>__BlockUntilKeyUp__ KeyCode::D</autogen>
            <autogen>__BlockUntilKeyUp__ KeyCode::F</autogen>
            <autogen>__BlockUntilKeyUp__ KeyCode::J</autogen>
            <autogen>__BlockUntilKeyUp__ KeyCode::K</autogen>
            <autogen>__BlockUntilKeyUp__ KeyCode::L</autogen>
            <autogen>__BlockUntilKeyUp__ KeyCode::SEMICOLON</autogen>
        </block>
    </item>

    <item>
        <name>Active G_GUIDE</name>
        <identifier>private.HAPPY_KEYOUT.G_GUIDE</identifier>
        <autogen>
            __KeyOverlaidModifier__
            KeyCode::G,

            @begin
            KeyCode::VK_CONFIG_FORCE_ON_notsave_G_GUIDE_FOR_ARROWS,
            KeyCode::VK_CONFIG_FORCE_ON_notsave_G_GUIDE_FOR_PAGE,
            @end

            @begin
            KeyCode::G,
            @end

            Option::KEYTOKEY_AFTER_KEYUP,
            KeyCode::VK_CONFIG_FORCE_OFF_notsave_G_GUIDE_FOR_ARROWS,
            KeyCode::VK_CONFIG_FORCE_OFF_notsave_G_GUIDE_FOR_PAGE
        </autogen>
    </item>

