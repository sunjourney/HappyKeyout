<?xml version="1.0"?>
<root>
    <item>
        <block>
            <item>
                <name>Activate SLASH_TOGGLE</name>
                <appendix>【 Simultaneous presses F and J => Toggle FASTTYPE MODE 】</appendix>
                <appendix>【 F => Toggle FANCY MODE 】</appendix>
                <appendix>【 J => Toggle JUNIOR MODE 】</appendix>
                <appendix>【 D => Toggle DIVINE MODE 】</appendix>
                <appendix>【 A => Toggle NUMPAD MODE 】</appendix>
                <appendix>【 Z => Toggle ZIPPO MODE 】</appendix>
                <identifier>private.HAPPY_KEYOUT.SLASH_TOGGLE</identifier>
                <autogen>
                    __KeyOverlaidModifier__
                    KeyCode::SLASH,

                    @begin
                    KeyCode::VK_CONFIG_SYNC_KEYDOWNUP_notsave_SLASH_TOGGLE_FOR_TOGGLE,
                    @end

                    @begin
                    KeyCode::SLASH
                    @end
                </autogen>
            </item>

            <item hidden="true">
                <name></name>
                <identifier vk_config="true">notsave.SLASH_TOGGLE_FOR_TOGGLE</identifier>

                <autogen>__BlockUntilKeyUp__ KeyCode::SLASH</autogen>

                <autogen>
                   __SimultaneousKeyPresses__
                    KeyCode::F, KeyCode::J, ModifierFlag::NONE,
                    KeyCode::VK_CONFIG_TOGGLE_notsave_FASTTYPE_MODE
                </autogen>

                <block>
                    <config_only>private.HAPPY_KEYOUT.F_FANCY</config_only>
                    <block>
                        <config_not>notsave.F_FANCY_FOR_CURSOR</config_not>
                        <autogen>
                            __KeyToKey__ KeyCode::F,
                            {{ SLASH_TOGGLE_FORCE_OFF_ALL }}
                            KeyCode::VK_CONFIG_FORCE_ON_notsave_F_FANCY_FOR_CURSOR
                        </autogen>
                    </block>
                    <autogen>
                        __KeyToKey__ KeyCode::F,
                        {{ SLASH_TOGGLE_FORCE_OFF_ALL }}
                    </autogen>
                </block>

                <block>
                    <config_only>private.HAPPY_KEYOUT.J_JUNIOR</config_only>
                    <block>
                        <config_not>notsave.J_JUNIOR_FOR_PROGRAMKEYS</config_not>
                        <autogen>
                            __KeyToKey__ KeyCode::J,
                            {{ SLASH_TOGGLE_FORCE_OFF_ALL }}
                            KeyCode::VK_CONFIG_FORCE_ON_notsave_J_JUNIOR_FOR_PROGRAMKEYS
                        </autogen>
                    </block>
                    <autogen>
                        __KeyToKey__ KeyCode::J,
                        {{ SLASH_TOGGLE_FORCE_OFF_ALL }}
                    </autogen>
                </block>

                <block>
                    <config_only>private.HAPPY_KEYOUT.D_DIVINE</config_only>
                    <block>
                        <config_not>notsave.D_DIVINE_FOR_MOUSE</config_not>
                        <autogen>
                            __KeyToKey__ KeyCode::D,
                            {{ SLASH_TOGGLE_FORCE_OFF_ALL }}
                            KeyCode::VK_CONFIG_FORCE_ON_notsave_D_DIVINE_FOR_MOUSE
                        </autogen>
                    </block>
                    <autogen>
                        __KeyToKey__ KeyCode::D,
                        {{ SLASH_TOGGLE_FORCE_OFF_ALL }}
                    </autogen>
                </block>

                <block>
                    <config_only>private.HAPPY_KEYOUT.A_PAD</config_only>
                    <block>
                        <config_not>notsave.A_PAD_FOR_NUMPAD</config_not>
                        <autogen>
                            __KeyToKey__ KeyCode::A,
                            {{ SLASH_TOGGLE_FORCE_OFF_ALL }}
                            KeyCode::VK_CONFIG_FORCE_ON_notsave_A_PAD_FOR_NUMPAD
                        </autogen>
                    </block>
                    <autogen>
                        __KeyToKey__ KeyCode::A,
                        {{ SLASH_TOGGLE_FORCE_OFF_ALL }}
                    </autogen>
                </block>

                <block>
                    <config_only>private.HAPPY_KEYOUT.P_POWER</config_only>
                    <block>
                        <config_not>notsave.P_POWER_FOR_PSD</config_not>
                        <autogen>
                            __KeyToKey__ KeyCode::P,
                            {{ SLASH_TOGGLE_FORCE_OFF_ALL }}
                            KeyCode::VK_CONFIG_FORCE_ON_notsave_P_POWER_FOR_PSD
                        </autogen>
                    </block>
                    <autogen>
                        __KeyToKey__ KeyCode::P,
                        {{ SLASH_TOGGLE_FORCE_OFF_ALL }}
                    </autogen>
                </block>

                <block>
                    <config_only>private.HAPPY_KEYOUT.Z_ZIPPO</config_only>
                    <block>
                        <config_not>notsave.Z_ZIPPO_FOR_CUSTOM_KEYS</config_not>
                        <autogen>
                            __KeyToKey__ KeyCode::Z,
                            {{ SLASH_TOGGLE_FORCE_OFF_ALL }}
                            KeyCode::VK_CONFIG_FORCE_ON_notsave_Z_ZIPPO_FOR_CUSTOM_KEYS
                        </autogen>
                    </block>
                    <autogen>
                        __KeyToKey__ KeyCode::Z,
                        {{ SLASH_TOGGLE_FORCE_OFF_ALL }}
                    </autogen>
                </block>
            </item>

            <item hidden="true">
                <name></name>
                <identifier vk_config="true">notsave.FASTTYPE_MODE</identifier>
                <autogen>__ShowStatusMessage__ FASTTYPE MODE</autogen>
            </item>
        </block>

        <name>HAPPYKEYOUT MODIFIER SWITCHERS</name>
        <!-- ASDFGHJKL;' -->
        <item>
            <name>Tranditional Modifiers Remapping</name>
            <appendix>【A】【S_SHIFT】【D】【F】【G】【H】【J】【K_OPT】【L_SHIFT】【SEMICOLON_CTRL】【QUOTE】</appendix>
            <appendix>【Z】【X】【C】【V】【B】【N】【M_META】【COMMA】【DOT】【SLASH】</appendix>
            <identifier>private.HAPPY_KEYOUT.HANDY_MODIFIER</identifier>
            <config_not>notsave.FASTTYPE_MODE</config_not>

            <autogen>__KeyOverlaidModifier__ KeyCode::S, KeyCode::SHIFT_L, KeyCode::S</autogen>
            <autogen>__KeyOverlaidModifier__ KeyCode::M, KeyCode::COMMAND_R, KeyCode::M</autogen>
            <autogen>__KeyOverlaidModifier__ KeyCode::K, KeyCode::OPTION_R, KeyCode::K</autogen>
            <autogen>__KeyOverlaidModifier__ KeyCode::L, KeyCode::SHIFT_R, KeyCode::L</autogen>
            <autogen>__KeyOverlaidModifier__ KeyCode::SEMICOLON, KeyCode::CONTROL_R, KeyCode::SEMICOLON,</autogen>

            <block>
                <autogen>__BlockUntilKeyUp__ KeyCode::S</autogen>
                <autogen>__BlockUntilKeyUp__ KeyCode::M</autogen>
                <autogen>__BlockUntilKeyUp__ KeyCode::K</autogen>
                <autogen>__BlockUntilKeyUp__ KeyCode::L</autogen>
                <autogen>__BlockUntilKeyUp__ KeyCode::SEMICOLON</autogen>
            </block>
        </item>

